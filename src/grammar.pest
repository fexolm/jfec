WHITESPACE = _{ " " | "\t" | "\r" | "\n" }

program = { SOI ~ decl* ~ EOI }

decl = { fn_decl }

fn_decl = { "fn" ~ id ~ "(" ~ param_list? ~ ")" ~ ret_typ ~ body }

ret_typ = { ("->" ~ id)? }

body = { "{" ~ stmt_list ~ "}" }

param  = { id ~ ":" ~ id }

param_list = { param ~ ("," ~ param)* }

stmt_list = { stmt* }

stmt = { "kek" }

id = { ASCII_ALPHA+ }
